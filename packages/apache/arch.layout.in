<Layout Arch>
	prefix:          %%PACMAN_ROOT%%/etc/httpd
	exec_prefix:     %%PACMAN_ROOT%%/usr
	bindir:          %%PACMAN_ROOT%%/usr/bin
	sbindir:         %%PACMAN_ROOT%%/usr/bin
	libdir:          %%PACMAN_ROOT%%/usr/lib/httpd
	libexecdir:      %%PACMAN_ROOT%%/usr/lib/httpd/modules
	mandir:          %%PACMAN_ROOT%%/usr/share/man
	sysconfdir:      %%PACMAN_ROOT%%/etc/httpd/conf
	datadir:         %%PACMAN_ROOT%%/usr/share/httpd
	installbuilddir: %%PACMAN_ROOT%%/usr/lib/httpd/build
	errordir:        %%PACMAN_ROOT%%/usr/share/httpd/error
	iconsdir:        %%PACMAN_ROOT%%/usr/share/httpd/icons
