	manualdir:       %%PACMAN_ROOT%%/usr/share/httpd/manual
	cgidir:          %%PACMAN_ROOT%%/srv/http/cgi-bin
	includedir:      %%PACMAN_ROOT%%/usr/include/httpd
	localstatedir:   %%PACMAN_ROOT%%/var
	runtimedir:      %%PACMAN_ROOT%%/run/httpd
